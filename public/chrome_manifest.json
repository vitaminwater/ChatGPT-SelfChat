{
  "name": "ChatGPT-SelfChat",
  "version": "1.0.1",
  "description": "Makes two instances of chatGPT chat together in two different browser windows.",
  "permissions": [
    "declarativeContent"
  ],
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "images/icon.png"
    }
  },
  "icons": {
    "128": "images/icon.png"
  }
}
