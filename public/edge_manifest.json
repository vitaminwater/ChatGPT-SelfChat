{
  "name": "datetime",
  "version": "1.0",
  "description": "Build a Datetime Extension!",
  "permissions": [
    "declarativeContent"
  ],
  "manifest_version": 2,
  "background": {
    "content_scripts": "background.js"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "images/icon.png"
    }
  },
  "icons": {
    "128": "images/icon.png"
  }
}
